class Solution {
public:
    bool isPalindrome(int n) {
        int original=n;
        int rev=0;
        int sum=0;
        while(n>0){
            rev=n%10;
            sum=sum*10+rev;
            n=n/10;
        }
        if(sum==original){
            return true;
        }return false;
    }
};
